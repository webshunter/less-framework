                        g = g.toLowerCase();
                    }
                    if(numcek == 0){
                        if(g == search){
                            numcek = 1;
                        }
                    }
                }
                if(numcek == 1){
                    return dat;
                }
            }else{
                if(dat != null){
                    if(typeof dat == 'number'){
                        var dats = dat.toString().toLowerCase();
                        if(dats  == search){
                            return dat
                        }
                    }else{
                        if(dat.toLowerCase() == search){
                            return dat
                        }
                    }
                }
            }
        })
    }else{
        return this;
    }
}

